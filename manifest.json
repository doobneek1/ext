{
  "manifest_version": 3,
  "name": "doobneek feat YourPeer",
  "version": "5.9.137",
  "description": "Injects text formatting buttons and redirect logic.",
  "permissions": [
    "storage",
    "tabs",
    "webNavigation",
    "scripting",
    "alarms"
  ],
  "host_permissions": [
    "https://gogetta.nyc/*",
    "https://gogetta.nyc/*",
    "https://test.gogetta.nyc/*",
    "https://www.test.gogetta.nyc/*",
    "https://yourpeer.nyc/*",
    "https://mail.google.com/*",
    "https://voice.google.com/*",
    "http://localhost:3210/*",
    "http://localhost:3000/*",
    "https://maps.googleapis.com/",
    "https://streetli-default-rtdb.firebaseio.com/*",
    "https://us-central1-streetli.cloudfunctions.net/*",
    "https://us-central1-streetli.cloudfunctions.net/locationNote1/*",
    "http://localhost:3210/*",
    "https://*.doobneek.org/*",
    "https://w6pkliozjh.execute-api.us-east-1.amazonaws.com/*",
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content-script.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "side_panel": {
      "default_path": "embed.html"
    },
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_title": "doobneek inline formatter"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "org_names.txt",
        "people_names.txt",
        "doobneekflyer.jpg",
        "yourpeerflyer.png",
        "doobneekflyerspan.jpg",
        "teamMapPinsPage.js",
        "tesseract.min.js",
        "leaflet/leaflet.js",
        "leaflet/leaflet.css",
        "leaflet/images/*",
        "serviceApiMonitor.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}